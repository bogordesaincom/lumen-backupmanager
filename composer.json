{
  "name": "bogordesain/backup-manager",
  "description": "Database backup manager seamlessly integrated with Lumen 4 or 5 with user-definable procedures and support for S3, Dropbox, FTP, SFTP, and more.",
  "authors": [
    {
      "name": "Shawn McCool",
      "email": "shawn@heybigname.com",
      "homepage": "https://shawnmc.cool"
    }
  ],
  "require": {
    "backup-manager/backup-manager": "^3.1",
    "php": "^7.3||^7.4||^8.0",
    "symfony/process": "^3||^4||^5||^6",
    "illuminate/support": "^5.5||^6||^7||^8||^9",
    "illuminate/container": "^5.5||^6||^7||^8||^9",
    "illuminate/console": "^5.5||^6||^7||^8||^9"
  },
  "require-dev": {
    "mockery/mockery": "dev-master",
    "satooshi/php-coveralls": "~0.6"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "Bogordesain\\BackupManager\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Bogordesain\\BackupManager\\Lumen55ServiceProvider"
      ]
    }
  },
  "license": "MIT"
}
